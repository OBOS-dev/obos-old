#pragma once

#include <types.h>
#include <stddef.h>

[[nodiscard]] PVOID operator new(size_t, void* ptr) noexcept;
[[nodiscard]] PVOID operator new[](size_t, void* ptr) noexcept;
VOID operator delete(PVOID, PVOID) noexcept;
VOID operator delete[](PVOID, PVOID) noexcept;